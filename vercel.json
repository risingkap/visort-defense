# Vercel deployment configuration for frontend
{
  "version": 2,
  "builds": [
    {
      "src": "visort-last/frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "visort-last/frontend/build/$1"
    }
  ]
}